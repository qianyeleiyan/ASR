}

.circle-button{
  font-size: 60rpx;
  color: white;
  text-align: center;
  border-radius: 100%;
}

.circle-view1{
    height: 120rpx;
    width: 120rpx;
    background-color: #19be6b;
    border-radius: 100%;
    position: fixed;
    bottom: 100rpx;
    right: 40rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    z-index: 9;
    border: 0 solid #ffffff;
    box-shadow: 4px 1px 1px #cccccc;
}
.circle-view2{
  height: 120rpx;
